# Environment variables for the frontend application
# Copy this file to .env.local and fill in your actual values

# Dynamic.xyz Environment ID
# Get this from your Dynamic.xyz dashboard
VITE_DYNAMIC_ENVIRONMENT_ID=your-environment-id-here

# Backend API URL
# In development, this should point to your local backend server
VITE_API_URL=http://localhost:3001

# App configuration
VITE_APP_NAME=Web3 Wallet Signer
VITE_APP_VERSION=1.0.0
